#set("title") { Talks }

#set("customCSS") { }

#set("body") {
    <div class="container">
        <!-- Content here -->
        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">Subject</th>
                    <th scope="col">Presenter</th>
                </tr>
            </thead>
            <tbody>
                #for(meeting in meetings) {
                    <tr><td colspan="2"><h4>Meeting of #(meeting.date)</h4></td></tr>
                    #for(talk in meeting.talks) {
                        <tr>
                            <td>#(talk.title)</td>
                            <td>#(talk.presenter)</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
}

#embed("base")
